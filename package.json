{
  "name": "explained",
  "displayName": "explained",
  "publisher": "jjavieralonso",
  "description": "Create a comment summarizing your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjavieralonso/explained.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
